{
  "name": "saashq",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 4002",
    "build": "next build",
    "start": "next start --port 4002",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install",
    "playwright:update": "playwright install && playwright install-deps",
    "test:e2e": "playwright test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --env=jsdom"
  },
  "dependencies": {
    "@boxyhq/saml-jackson": "^1.9.7",
    "@heroicons/react": "2.0.18",
    "@hookform/resolvers": "2.9.11",
    "@next-auth/prisma-adapter": "1.0.6",
    "@novu/node": "^0.15.0",
    "@novu/notification-center": "^0.15.0",
    "@prisma/client": "^4.14.1",
    "@retracedhq/logs-viewer": "2.4.6",
    "@retracedhq/retraced": "0.6.3",
    "@stripe/stripe-js": "^1.54.0",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.2",
    "cookies-next": "2.1.1",
    "crypto-js": "^4.1.1",
    "formik": "2.2.9",
    "i18next": "22.4.15",
    "knex": "^2.5.0",
    "moment": "2.29.4",
    "next": "^13.4.9",
    "next-auth": "4.22.1",
    "next-i18next": "13.2.2",
    "nodemailer": "6.9.2",
    "react": "18.2.0",
    "react-daisyui": "^2.5.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-i18next": "12.3.1",
    "react-icons": "4.8.0",
    "react-toggle-dark-mode": "^1.1.1",
    "stripe": "^12.8.0",
    "svix": "0.85.1",
    "swr": "2.1.5",
    "tabler-icons-react": "^1.56.0",
    "usehooks-ts": "^2.9.1",
    "uuid": "9.0.0",
    "yup": "1.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "1.27.1",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/bcryptjs": "2.4.2",
    "@types/crypto-js": "4.1.1",
    "@types/jest": "29.2.3",
    "@types/mocha": "10.0.1",
    "@types/node": "18.14.2",
    "@types/nodemailer": "6.4.6",
    "@types/react": "18.2.8",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.59.2",
    "daisyui": "2.52.0",
    "eslint": "8.37.0",
    "eslint-config-next": "13.4.7",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.6.1",
    "lint-staged": "13.2.3",
    "postcss": "8.4.25",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prisma": "^4.14.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,ts,css,md}": "prettier --write"
  }
}
